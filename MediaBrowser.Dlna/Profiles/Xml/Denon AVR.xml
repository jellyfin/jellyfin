<?xml version="1.0"?>
<Profile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Denon AVR</Name>
  <Identification>
    <FriendlyName>Denon:\[AVR:.*</FriendlyName>
    <Manufacturer>Denon</Manufacturer>
    <Headers />
